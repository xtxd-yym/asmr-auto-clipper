        "target": "es2018",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": false
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}